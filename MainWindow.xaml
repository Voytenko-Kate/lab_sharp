<Window x:Class="PodsystemaFizLicz.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PodsystemaFizLicz"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Menu>
            <MenuItem x:Name="Persons" Header="Владельцы документов" Click="Persons_OnClick"></MenuItem>
            <MenuItem x:Name="Citizen" Header="Физические лица" Click="Citizen_OnClick"></MenuItem>
            <MenuItem x:Name="Documents" Header="Документы" Click="Documents_OnClick"></MenuItem>
        </Menu>
    </Grid>
</Window>
